-- Function to log fake activities
local function logActivity()
    while true do
        for _, user in ipairs(users) do
            local logEntry = Instance.new("TextLabel")
            logEntry.Parent = liveLog
            logEntry.Text = user.name .. " Duped " .. user.huges .. " Huges"
            logEntry.Size = UDim2.new(1, 0, 0, 30)
            logEntry.Font = Enum.Font.Cartoon
            logEntry.TextSize = 24
            logEntry.TextColor3 = Color3.new(1, 1, 1)
            logEntry.BackgroundTransparency = 1
            liveLog.CanvasSize = UDim2.new(0, 0, 0, liveLogLayout.AbsoluteContentSize.Y)
            wait(5)
        end
    end
end

-- Function to start duping (Placeholder for actual functionality)
local function startDupingFunction()
    -- Your custom duping function logic here
end

-- Function to start duping when button is clicked
startButton.MouseButton1Click:Connect(function()
    startDupingFunction()
end)

-- Function to activate the script when button is clicked
activateButton.MouseButton1Click:Connect(function()
    statusLabel.Text = "Status: Active"
    statusLabel.TextColor3 = Color3.new(0, 1, 0)
    coroutine.wrap(logActivity)()
end)

refreshDropdown()
