dropdown.BackgroundColor3 = Color3.new(0.2, 0.2, 0.2)
dropdown.BorderSizePixel = 0

local dropdownMenu = Instance.new("Frame")
dropdownMenu.Parent = mainFrame
dropdownMenu.Size = UDim2.new(1, 0, 0, 180)
dropdownMenu.Position = UDim2.new(0, 0, 0, 180)
dropdownMenu.BackgroundColor3 = Color3.new(0.2, 0.2, 0.2)
dropdownMenu.Visible = false
dropdownMenu.BorderSizePixel = 0

local dropdownOptions = {"Exclusive Egg", "Option 2", "Huge Happy Computer", "Huge Cyber Agony"}

local function refreshDropdown()
    dropdownMenu:ClearAllChildren()
    for i, option in ipairs(dropdownOptions) do
        local optionButton = Instance.new("TextButton")
        optionButton.Parent = dropdownMenu
        optionButton.Text = option
        optionButton.Size = UDim2.new(1, 0, 0, 60)
        optionButton.Font = Enum.Font.Cartoon
        optionButton.TextSize = 32
        optionButton.TextColor3 = Color3.new(1, 1, 1)
        optionButton.BackgroundColor3 = Color3.new(0.2, 0.2, 0.2)
        optionButton.BorderSizePixel = 0
        optionButton.MouseButton1Click:Connect(function()
            dropdown.Text = option
            dropdownMenu.Visible = false
        end)
    end
end

dropdown.MouseButton1Click:Connect(function()
    dropdownMenu.Visible = not dropdownMenu.Visible
end)

activateButton.Name = "ActivateButton"
activateButton.Parent = mainFrame
